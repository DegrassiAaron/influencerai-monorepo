{
  "name": "@influencerai/api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "start": "node dist/src/main.js",
    "prepare": "pnpm run prisma:generate",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "pnpm run test:unit",
    "test:unit": "node ../../scripts/run-jest.mjs --config jest.config.js",
    "test:integration": "node ../../scripts/run-jest.mjs --config jest.e2e.config.js --runInBand",
    "test:e2e": "pnpm run test:integration",
    "test:e2e:db:reset": "node scripts/test-db-reset.mjs",
    "test:watch": "jest --watch",
    "start:dev": "pnpm dev",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:db:push": "prisma db push",
    "pretest": "pnpm run prisma:generate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.908.0",
    "@aws-sdk/s3-request-presigner": "^3.908.0",
    "@fastify/static": "^7.0.4",
    "@influencerai/core-schemas": "workspace:*",
    "@influencerai/prompts": "workspace:*",
    "@nestjs/bullmq": "^10.2.1",
    "@nestjs/common": "^10.4.8",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.4.8",
    "@nestjs/swagger": "^8.0.7",
    "@prisma/client": "^6.17.1",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.38.2",
    "fastify": "^5.2.3",
    "ioredis": "^5.8.1",
    "nestjs-pino": "^4.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino": "^9.4.0",
    "pino-pretty": "^11.3.0",
    "rxjs": "^7.8.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@influencerai/sdk": "workspace:*",
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.37.0",
    "jest": "^29.7.0",
    "nock": "^13.5.5",
    "prisma": "^6.17.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "undici": "^6.19.8"
  }
}
